{
    "name": "mpenna/squirrel",
    "description": "Laravel package that automatically caches and retrieves models when querying records using Eloquent ORM",
    "keywords": ["laravel", "eloquent", "cache", "query", "orm", "automatic"],
    "license": "MIT",
    "authors": [{
        "name": "David Peace",
        "email": "peaceman@gmail.com"
    }],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.*|5.*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0",
        "illuminate/database": "4.*|5.*"
    },
    "replace": {
        "davidmpeace/squirrel": "^0.*"
    },
    "autoload": {
        "psr-4": {
            "Eloquent\\Cache\\": "src/"
        }
    }
}